syntax "json" "\.json$"
header "^\{$"

color yellow   "[[:<:]][-]?[1-9][0-9]*([Ee][+-]?[0-9]+)?[[:>:]]"  "[[:<:]][-]?[0](\.[0-9]+)?[[:>:]]"
color brightwhite  "[[:<:]]null[[:>:]]"
color brightyellow "[[:<:]](true|false)[[:>:]]"
color green ""(\\.|[^"])*"|'(\\.|[^'])*'"
color magenta "\"(\\"|[^"])*\"[[:space:]]*:"  "'(\'|[^'])*'[[:space:]]*:"
color black,cyan   "\\u[0-9a-fA-F]{4}|\\[bfnrt'"/\\]"
color ,cyan "[[:space:]]+$"
color brightwhite,red    "	+ +| +	+"
# +EXTRALINT
