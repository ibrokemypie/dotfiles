syntax "mpdconf" "mpd\.conf$"

color brightwhite "[[:<:]](user|group|bind_to_address|host|port|plugin|name|type)[[:>:]]"
color brightwhite "[[:<:]]((music|playlist)_directory|(db|log|state|pid|sticker)_file)[[:>:]]"
color brightmagenta "^(input|audio_output|decoder)[[:space:]]*\{|\}"
color green ""(\\.|[^"])*"|'(\\.|[^'])*'"
color cyan "(^|[[:space:]])#([^{].*)?$"
color ,cyan "[[:space:]]+$"
color brightwhite,red    "	+ +| +	+"
# +EXTRALINT
